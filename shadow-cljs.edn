{:deps     {:aliases [:dev]}
 :dev-http {8000 "classpath:public"}
 :builds   {:main {:target     :browser
                   :output-dir "resources/public/js/main"
                   :asset-path "/js/main"
                   :modules    {:main {:init-fn todoaday.client/init
                                       :entries [todoaday.client]}}
                   :devtools   {:after-load todoaday.client/refresh
                                :preloads   [com.fulcrologic.fulcro.inspect.preload]}}}}